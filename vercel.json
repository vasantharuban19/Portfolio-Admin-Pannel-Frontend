{
    "headers": [
        {
            "source": "/assets/*.js",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "destination": "/index.html",
            "statusCode": 200
        }
    ]
}